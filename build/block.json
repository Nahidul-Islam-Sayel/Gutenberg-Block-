{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "techearty/boilerplate",
  "version": "0.1.0",
  "title": "sayels",
  "category": "text",
  "description": "Example block scaffolded with Create Block tool.",
  "keywords": [
    "text",
    "paragraph",
    "box"
  ],
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "gradients": true
    },
    "spacing": {
      "padding": true
    }
  },
  "textdomain": "text-box",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "text": {
      "type": "string",
      "source": "html",
      "selector": "h4"
    },
    "alignment": {
      "type": "string",
      "default": "left"
    },
    "shadow": {
      "type": "boolean",
      "default": false
    },
    "backgroundColor": {
      "type": "string",
      "default": "strong-magenta"
    },
    "textColor": {
      "type": "string",
      "default": "very-light-gray"
    },
    "gradient": {
      "type": "string",
      "default": "green-to-yellow"
    },
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "#f03"
        },
        "spacing": {
          "padding": {
            "top": "50px",
            "right": "50px",
            "bottom": "50px",
            "left": "50px"
          }
        }
      }
    }
  },
  "viewScript": "file:./view.js"
}